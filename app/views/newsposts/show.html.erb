<p id="notice"><%= notice %></p>
<p>
  <strong>Title:</strong>
  <%= @newspost.title %>
</p>
<p>
  <strong>Content:</strong>
  <%= @newspost.content %>
</p>

<p>
  <strong>Poster:</strong>
  <%= @newspost.poster %>
</p>
<p>
  <strong>Tags: </strong>
  <ul>
    <% @newspost.tags.each do |tag| %>
      <li><%= tag.name %></li>
    <% end %>
  </ul>
</p>
<%= link_to 'Edit', edit_newspost_path(@newspost) %> |
<%= link_to 'Back', newsposts_path %>
